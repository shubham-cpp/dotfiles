{
  "coc.preferences.bracketEnterImprove": true,
  "codeLens.enable": true,
  "codeLens.position": "right_align",
  "Lua.telemetry.enable": false,
  "Lua.completion.callSnippet": "Replace",
  "sumneko-lua.enableNvimLuaDev": true,
  "Lua.diagnostics.disable": ["lowercase-global"],
  "Lua.diagnostics.globals": ["vim", "describe"],
  "Lua.hint.arrayIndex": "Enable",
  "Lua.hint.setType": true,
  "Lua.runtime.version": "LuaJIT",
  "Lua.semantic.keyword": true,
  "Lua.workspace.preloadFileSize": 1000,
  "tailwindCSS.headwind.runOnSave": true,
  "volar.autoCreateQuotes": true,
  "volar.autoClosingTags": true,
  "volar.autoCompleteRefs": true,
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "go.goplsOptions": {
    "completeUnimported": true,
    "gofumpt": true,
    "semanticTokens": true,
    "hints": {
      "assignVariableTypes": true,
      "constantValues": true,
      "parameterNames": true,
      "rangeVariableTypes": true
    },
    "analyses": {
      "nilness": true,
      "unusedparams": true
    }
  }
}
